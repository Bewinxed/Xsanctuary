{
  "name": "xsanctuary",
  "description": "Show country flags for Twitter/X users based on their location",
  "private": true,
  "version": "1.0.2",
  "type": "module",
  "author": {
    "name": "Omar Al Matar",
    "email": "bewinxed@gmail.com",
    "url": "https://bewinxed.com"
  },
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "build:all": "bun run build && bun run build:firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "zip:all": "bun run zip && bun run zip:firefox",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "typecheck": "svelte-check --tsconfig ./tsconfig.json",
    "prepush": "bun run check && bun run build:all",
    "postinstall": "wxt prepare",
    "prepare": "git config core.hooksPath .githooks 2>/dev/null || true"
  },
  "devDependencies": {
    "@internationalized/date": "^3.8.1",
    "@lucide/svelte": "^0.544.0",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/table-core": "^8.20.5",
    "@tsconfig/svelte": "^5.0.5",
    "@wxt-dev/auto-icons": "^1.1.0",
    "@wxt-dev/module-svelte": "^2.0.3",
    "autoprefixer": "^10.4.22",
    "bits-ui": "^2.14.4",
    "clsx": "^2.1.1",
    "embla-carousel-svelte": "^8.6.0",
    "formsnap": "^2.0.1",
    "layerchart": "2.0.0-next.27",
    "lucide-svelte": "^0.554.0",
    "mode-watcher": "^1.1.0",
    "paneforge": "^1.0.2",
    "postcss": "^8.5.6",
    "svelte": "^5.38.6",
    "svelte-check": "^4.3.1",
    "svelte-sonner": "^1.0.5",
    "sveltekit-superforms": "^2.26.1",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.1.17",
    "tslib": "^2.8.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.2",
    "vaul-svelte": "^1.0.0-next.7",
    "wxt": "^0.20.6"
  },
  "dependencies": {
    "country-flag-icons": "^1.6.4",
    "onnxruntime-web": "^1.23.2"
  }
}
